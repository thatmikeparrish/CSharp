create table Cohort (
	Id	integer not null primary key autoincrement,
	Name	text not null
);

create table Exercise (
	Id	integer not null primary key autoincrement,
	Name	text not null,
	Language text not null
);

create table Student (
	Id	integer not null primary key autoincrement,
	firstName text not null,
	lastName text not null,
	slackHandle text not null,
	StudentCohort integer not null,
	foreign key(StudentCohort) references Cohort(Id)
);

create table Instructor (
	Id	integer not null primary key autoincrement,
	firstName text not null,
	lastName text not null,
	slackHandle text not null,
	InstructorCohort integer not null,
	foreign key(InstructorCohort) references Cohort(Id)
);

